$def with (toUser, fromUser, dyxx, num)
    <xml>
        <ToUserName><![CDATA[$toUser]]></ToUserName>
        <FromUserName><![CDATA[$fromUser]]></FromUserName>
        <CreateTime>12345678</CreateTime>
        <MsgType><![CDATA[news]]></MsgType>
        <ArticleCount>$num</ArticleCount>
        <Articles>
            $for m in range(num):
            <item>
                    <Title>
                         OJ:$dyxx[m][0]
                         name:$dyxx[m][1]
                         starttime:$dyxx[m][2]
                    </Title>
            </item>
        </Articles>
    </xml>